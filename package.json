{
    "name": "english",
    "private": true,
    "scripts": {
        "start": "npm-run-all --parallel start:client start:server",
        "start:client": "ieremeev-app-client-dev",
        "start:server": "npm run migrate && nodemon src/server/",
        "start:prod": "npm-run-all --parallel start:client:prod start:server:prod",
        "start:client:prod": "ieremeev-app-client-prod",
        "start:server:prod": "npm run migrate && node src/server/",
        "migrate": "sequelize db:migrate",
        "build": "ieremeev-app-build",
        "lint": "ieremeev-app-lint",
        "format": "ieremeev-app-format",
        "test": "ieremeev-app-test --watch",
        "test:prod": "jest -c='{\"testMatch\":[\"**/?(*.)prod.js\"]}' src/server/test.prod.js",
        "testcafe": "testcafe 'chrome:headless --autoplay-policy=no-user-gesture-required' src/testcafe/**/*.page.js",
        "testcafe:live": "testcafe-live 'chrome --autoplay-policy=no-user-gesture-required' src/testcafe/**/*.page.js",
        "testcafe:db": "node src/bin/restoreDb.js",
        "testcafe:prod": "testcafe 'chrome:headless --autoplay-policy=no-user-gesture-required' src/testcafe/**/*.prod.js",
        "testcafe:prod:live": "testcafe-live 'chrome --autoplay-policy=no-user-gesture-required' src/testcafe/**/*.prod.js",
        "user:add": "node src/bin/addUser.js",
        "analyze": "ieremeev-app-analyze-bundle",
        "duplicate": "ieremeev-app-duplicate",
        "check": "ieremeev-app-lint && ieremeev-app-test",
        "deploy": "shipit production deploy",
        "precommit": "ieremeev-app-format -s && ieremeev-app-lint -s && ieremeev-app-test -s"
    },
    "peerDependencies": {
        "enzyme": "*"
    },
    "directories": {
        "lib": "src/server"
    },
    "dependencies": {
        "@ieremeev/axios": "^1.1.2",
        "@ieremeev/boilerplate": "^3.0.1",
        "@ieremeev/button": "^2.0.3",
        "@ieremeev/formik": "^1.5.1",
        "@ieremeev/hidden-block": "^1.0.3",
        "@ieremeev/icons": "^1.2.2",
        "@ieremeev/loader": "^1.2.2",
        "@ieremeev/notification": "^2.0.6",
        "@ieremeev/semantic-ui": "^1.0.2",
        "@ieremeev/table": "^2.4.2",
        "@ieremeev/tree": "^1.0.6",
        "aws-sdk": "^2.54.0",
        "babel-polyfill": "^6.23.0",
        "body-parser": "^1.17.2",
        "child-process-promise": "^2.2.1",
        "classnames": "^2.2.5",
        "colors": "1.1.2",
        "cors": "^2.8.3",
        "date-fns": "^1.28.5",
        "dotenv": "^4.0.0",
        "express-fileupload": "^1.0.0",
        "feathers": "^2.1.2",
        "feathers-authentication": "^0.7.12",
        "feathers-configuration": "^0.3.3",
        "feathers-errors": "^2.8.0",
        "feathers-hooks": "^1.8.1",
        "feathers-rest": "^1.7.2",
        "feathers-sequelize": "^1.4.5",
        "fs-extra": "^7.0.0",
        "history": "^4.7.2",
        "howler": "^2.0.15",
        "js-cookie": "^2.2.0",
        "lodash": "^4.17.4",
        "md5": "^2.2.1",
        "mysql": "^2.13.0",
        "nodemon": "^1.17.2",
        "npm-run-all": "^4.1.2",
        "passport": "^0.3.2",
        "prop-types": "^15.5.10",
        "randomstring": "^1.1.5",
        "react": "^16.6.3",
        "react-dnd": "^2.1.4",
        "react-dnd-html5-backend": "^2.1.2",
        "react-dom": "^16.6.3",
        "react-redux": "^5.0.5",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "recompose": "^0.23.4",
        "redux": "^3.6.0",
        "redux-actions": "^2.0.3",
        "redux-immutable-state-invariant": "^2.0.0",
        "redux-thunk": "^2.2.0",
        "request-promise-native": "^1.0.4",
        "reselect": "^3.0.1",
        "semantic-ui-react": "^0.81.3",
        "sequelize": "^3.30.4",
        "sequelize-cli": "^2.7.0",
        "serve-favicon": "^2.4.3",
        "sharp": "^0.21.0",
        "string-template": "^1.0.0",
        "temp": "^0.8.3",
        "validator": "^7.0.0",
        "winston": "^2.3.1"
    },
    "devDependencies": {
        "@ieremeev/app": "^3.2.16",
        "babel-core": "^7.0.0-bridge.0",
        "inquirer": "^6.2.0",
        "shipit-cli": "4.1.1",
        "shipit-deploy": "4.1.1",
        "shipit-utils": "1.4.0",
        "supertest": "^3.3.0",
        "testcafe": "^0.23.2",
        "testcafe-live": "^0.1.3",
        "testcafe-react-selectors": "^2.1.0"
    }
}
